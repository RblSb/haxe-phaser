package phaser.types.input.keyboard;

typedef CursorKeys = {
	/**
		A Key object mapping to the UP arrow key.
	**/
	var up : phaser.input.keyboard.Key;
	/**
		A Key object mapping to the DOWN arrow key.
	**/
	var down : phaser.input.keyboard.Key;
	/**
		A Key object mapping to the LEFT arrow key.
	**/
	var left : phaser.input.keyboard.Key;
	/**
		A Key object mapping to the RIGHT arrow key.
	**/
	var right : phaser.input.keyboard.Key;
	/**
		A Key object mapping to the SPACE BAR key.
	**/
	var space : phaser.input.keyboard.Key;
	/**
		A Key object mapping to the SHIFT key.
	**/
	var shift : phaser.input.keyboard.Key;
};